    "babel-preset-es2017": "^6.24.1",
    "chai": "^4.1.2",
    "child-process-promise": "^2.2.1",
    "esca-scripts": "^0.12.1",
    "mocha": "^4.0.1",
    "opn-cli": "^3.1.0",
    "prompt-confirm": "^1.2.0",
    "serverless": "^1.23.0",
    "serverless-offline": "^3.16.0",
